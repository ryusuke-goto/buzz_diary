<% if @max_liked_diaries.present? %>
  <div id="diaries">
    <div class="text-2xl">
      いいねを最ももらった日記は！
    </div>
    <%= render partial: "diaries/diary", collection: @max_liked_diaries %>
  </div>
<% else %>
  <div class="flex items-center justify-center text-2xl">
    <%= t("diaries.no_result") %>
  </div>
<% end %>