<h2>テストユーザー ログイン</h2>

<%= form_with url: test_sign_in_path, method: :post, local: true do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password, autocomplete: "current-password" %>
  </div>

  <div class="actions">
    <%= form.submit "ログイン" %>
  </div>
<% end %>
